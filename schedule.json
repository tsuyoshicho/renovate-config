{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":timezone(Asia/Tokyo)",
    ":preserveSemverRanges"
  ],
  "semanticCommits": "enabled",
  "labels": [
    "renovate"
  ],
  "npm": {
    "enabled": true
  },
  "lockFileMaintenance": {
    "enabled": true
  },
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "schedule": [
    "every weekend"
  ]
}
